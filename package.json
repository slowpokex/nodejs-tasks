    "babel-register": "^6.26.0",
    "cross-env": "^5.1.4",
    "eslint": "^4.8.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "faker": "^4.1.0",
    "mocha": "^5.0.5",
    "nodemon": "^1.12.0",
    "nyc": "^11.6.0",
    "standart": "^6.1.0",
    "supertest": "^3.0.0"
  }
}

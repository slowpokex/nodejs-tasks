{
  "name": "node_tasks_main",
  "version": "1.0.0",
  "main": "init.js",
  "scripts": {
    "lint": "standard ./src --fix",
    "test": "npm run test:unit && npm run test:int",
    "test:unit": "npm run mocha ./test/**/*.unit.js",
    "test:int": "npm run mocha ./test/**/*.int.js",
    "test:cov": "nyc npm run test:int",
    "mocha": "cross-env NODE_ENV=test mocha --exit --timeout 12000 --require babel-register --recursive",
    "clean": "rimraf dist || mkdir dist",
    "prestart": "npm run clean && npm run -s build",
    "start": "node dist/index.js",
    "build": "npm run clean && mkdir dist && babel src -s -D -d dist",
    "dev": "cross-env NODE_ENV=development DEBUG=node_app nodemon --exec babel-node ./src/index.js"
  },
  "author": "Mikita Isakau <harley26.gold@gmail.com>",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "bcrypt": "^1.0.3",
    "chai": "^4.1.2",
    "connect-flash": "^0.1.1",
    "csv-parse": "^1.2.3",
    "dotenv": "^5.0.1",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "express-validation": "^1.0.2",
    "helmet": "^3.9.0",
    "http-status": "^1.0.1",
    "joi": "^13.1.2",
    "jsonwebtoken": "^8.1.0",
    "lodash": "^4.17.4",
    "minimist": "^1.2.0",
    "mongodb": "^2.2.33",
    "mongoose": "^4.12.5",
    "morgan": "^1.9.0",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth2": "^0.1.6",
    "passport-google-oauth20": "^1.0.0",
    "passport-http-bearer": "^1.0.1",
    "passport-local": "^1.0.0",
    "passport-twitter": "^1.0.4",
    "request": "^2.83.0",
    "split2": "^2.2.0",
    "supertest-as-promised": "^4.0.2",
    "swagger": "^0.7.5",
    "through2": "^2.0.3"

{
    "extends" : "airbnb",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": false
        }
    },
    "rules": {
        "import/extensions": "off",
        "jsx-a11y/href-no-hash": "off",
        "no-console": "off"
    }
}